<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:drawable="@drawable/thumb_false">
        <shape android:shape="rectangle">
            <size android:height="60dp" android:width="60dp"/>
            <corners android:radius="30dp"/>
            <solid android:color="#eeeeee"/>
            <stroke android:width="1dp" android:color="#666666"/>
        </shape>
    </item>
    <item android:state_checked="true" android:drawable="@drawable/thumb_true">
        <shape android:shape="rectangle">
            <size android:height="60dp" android:width="60dp"/>
            <corners android:radius="30dp"/>
            <solid android:color="#eeeeee"/>
            <stroke android:width="1dp" android:color="#33da33"/>
        </shape>
    </item>
</selector>