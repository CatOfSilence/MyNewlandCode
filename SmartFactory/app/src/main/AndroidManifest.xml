<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.jsit.smartfactory">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".tools.SmartFactoryApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SettingActivity"
            android:label="@string/globle_param_setting" />
        <activity android:name=".DataChartActivity" />
        <activity android:name=".WarnListActivity" />
        <activity android:name=".MonitorActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".LanguageActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".BaseActivity"></activity>
    </application>

</manifest>