<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--ShowData属性-->
    <declare-styleable name="ShowDataView">
        <attr name="title" format="string"></attr>
        <attr name="content" format="string"></attr>
        <attr name="bg">
            <enum name="green" value="0" />
            <enum name="red" value="1" />
            <enum name="yellow" value="2" />
            <enum name="blue" value="3" />
        </attr>
    </declare-styleable>
</resources>